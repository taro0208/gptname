　    "competitive_analysis":"両者の特長や戦略を取り上げて、どちらに軍配が上がるかを検討する",
      "personality_compatibility":"int",
　　　"role_compatibility":"int",
　　　"personality_compatibilityReason": "予想した理由を詳細に実況",
　　　"role_compatibilityReason": "予想した理由を詳細に実況", 
      "oneLastWord":"最後に一言"
}
