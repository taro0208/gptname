　　"advantage":{
               "personality_compatibility":"凄く有利、有利、不利、凄く不利",
               "role_compatibility":"凄く有利、有利、不利、凄く不利",
         }
                "advantageReason": "予想した理由を詳細に実況",
                "role_compatibilityReason": "予想した理由を詳細に実況",
               "oneLastWord":"最後に一言"
}
